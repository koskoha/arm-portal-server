{
	"presets": [
		"@babel/typescript",
		"power-assert",
		[
			"@babel/env",
			{
				"targets": {
					"node": true
				}
			}
		]
	],
	"plugins": [
		"babel-plugin-transform-typescript-metadata",
		["@babel/plugin-proposal-decorators", { "legacy": true }],
		["@babel/plugin-proposal-class-properties", { "loose": true }]
	]
}
